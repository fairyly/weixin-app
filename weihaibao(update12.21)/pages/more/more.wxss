.more{
    display: flex;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    padding-top: 10px;
}
.more-content{
    width: 95%;
    margin: 0px auto;
    text-align: center;
}
.more-contentul li{
    list-style: none;
    float: left;
    width: 48%;
    margin-left: 1%;
    margin-right: 1%;
    padding-top: 10px;
    padding-bottom: 5px;
    text-align: center;
    margin-bottom: 10px;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-shadow: 0 1px 1px -1px rgba(0, 0, 0, 0.3),0 -1px 1px rgba(0, 0, 0, 0.3); 
    border-radius: 0.1rem;
}
.more-contentul li image{
    width: 80%;
}