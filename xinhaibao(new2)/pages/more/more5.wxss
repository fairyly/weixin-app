.watermark{
    width: 100%;
    height: 1980rpx;
    background-color: #232323;
}
.add{
    position: absolute;
    left: 50%;
    top: 50%;
    width: 10rem;
    height: 10rem;
    margin-left: -5rem;
    margin-top: -5rem;
}
.add image{
   width: 100%;
   height: 100%;
}
.markmain{
    position: relative;
    top: 50%;
    overflow: hidden;
    padding-bottom: 8rem;
    text-align: center;
    -webkit-transition: margin 0.3s;
    transition: margin 0.3s;
}
#addpic{
    width: 100%;
    height: 720rpx;
}
.addmark{
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    border: none;
    z-index: 100;
}
.listwrap{
    position: relative;
    width: 10rem;
    top: 0;
    left: 0
}
.listarea{
    width: 10rem;
    position: absolute;
    top: 1rem;
    left: 5rem;
}
.listpimg image{
    width: 20px;
    height: 20px;
    margin-right: .17rem;
    vertical-align: middle;
}
.shengchengmark{
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    border: none;
    z-index: 100;
}
.watermark li,ol,ul{
    list-style: none;
}
.masklist{
    display: block;
    padding: 2% 0 22% 0;
}
.masklist li {
    display: block;
    position: relative;
    left: 0;
    top: 0;
    float: left;
    margin-left: 2%;
    margin-bottom: 2%;
    width: 47%;
    padding-bottom: 47%;
    height: 0;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center center;
    background-color: #454545;
    border-radius: 0.3rem;
    overflow: hidden;
}
.masklist li div {
    left: -5%;
    top: -6%;
    -webkit-transform: scale(.9);
    transform: scale(.9);
}
.mod-mark{
    width: 100%;
}
.mod-mark image{
    width: 80%;
    height: 320rpx;
}
.mod-mark text{
    display: block;
    font-size: 30rpx;
    font-weight: bold;
    overflow: hidden;
    text-overflow: clip;
    white-space: nowrap;
}
.mod-mark p{
    display: block;
    font-size: 23rpx;
    overflow: hidden;
    text-overflow: clip;
    white-space: nowrap;
}

.addstepfour{
    margin-bottom: 4rem;
}
.markform {
    margin: .89rem .9rem 0 .9rem;
}
.markform input {
    color: #999;
    border: none;
    background: 0 0;
    text-align: left;
}
.markform input.h3 {
    font-size: 1rem;
    height: 2rem;
    line-height: 2rem;
    width: 100%;
    border-bottom: #777 solid 1px;
}
.markform p.h3 {
    font-size: 0.8rem;
    color: #666;
    height: 2rem;
    line-height: 2rem;
    margin-bottom: .935rem;
}
.markform .l {
    position: relative;
    margin-bottom: 1rem;
    height: 2rem;
    font-size: 0.8rem;
    border-bottom: #444 solid 1px;
}
.markform .l .icon {
    background: #232323;
    position: absolute;
    left: -.85rem;
    bottom: -.17rem;
    width: 3.0rem;
    height: 1.83rem;
    line-height: 1.83rem;
    text-align: center;
}
.markform .l.focus .icon span {
    position: relative;
    display: inline-block;
    vertical-align: middle;
    width: 1rem;
    height: 1rem;
    line-height: 1rem;
    border-radius: 1rem;
    margin-top: -.51rem;
    background: #ccc;
    border: none;
    color: #000;
    font-size: 1.19rem;
    text-align: center;
}
.markform .l .icon span {
    display: inline-block;
    vertical-align: middle;
    width: .48rem;
    height: .48rem;
    border: .153rem solid #5f5f5f;
    border-radius: 0.34rem;
    text-indent: -9rem;
    margin-top: -.51rem;
}
.markform .l.focus .icon span:before {
    position: absolute;
    left: .35rem;
    top: .103rem;
    content: "";
    width: .255rem;
    height: .60rem;
    border-bottom: .11rem solid #000;
    border-right: .11rem solid #000;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
}
.markform .l .icon1 image{
    width: 20px;
    height: 20px;
    vertical-align: middle;
}
.markform .l.focus .icon1 image{
    width: 20px;
    height: 20px;
    vertical-align: middle;
}
.markform .l .icon1 {
    position: absolute;
    left: 2.1235rem;
    top: .553rem;
    width: 1.125rem;
    height: 1.125rem;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 95%;
}
.markform .l input {
    height: 2.635rem;
    width: 15rem;
    margin-left: 3.5rem;
    font-size: 0.8rem;
}
.markform .l input::-webkit-input-placeholder {
    color: #666;
}
.markform .l input:-moz-placeholder,.markform .l input::-moz-placeholder {
    color: #666;
}

.markform .l input:-ms-input-placeholder {
    color: #666;
}

.markform .l.focus input::-webkit-input-placeholder {
    color: #ababab;
}

.markform .l.focus input:-moz-placeholder,.markform .l.focus input::-moz-placeholder {
    color: #ababab;
}
.markform .l.focus input{
     color: #ababab;
}
.markform .l.focus input:-ms-input-placeholder {
    color: #ababab;
}
.create-mark {
    position: fixed;
    bottom: 0;
    width: 100%;
    border: none;
    z-index: 100;
}