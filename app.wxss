/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
} 
/*导航*/
.index-nav {
    width: 100%;
    height: 40px;
    line-height: 40px;
    background: #ffffff;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 111;
    box-shadow:0px 1px 1px 1px #efefef;
}
.index-nav image {
    margin: 0px;
}
.index-nav .nav-menu-icon {
    display: inline-block;
    vertical-align: middle;
    width: 20px;
    height: 20px;
    margin-left: 20px;
}

.index-nav .index-company-logo {
    display: inline-block;
    vertical-align: middle;
    width: 30px;
    height: 30px;
    margin-left: 32%;
}

.index-nav span {
    font-size: 16px;
    color:#333;
    margin-left: 5px;
}
