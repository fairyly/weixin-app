.cal-content{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    padding-top: 10px;
}
.cal-container{
    width: 720rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: right;
    border-radius: 3px;
    margin: 0px auto;
    border: 1px solid #4eff22;
    margin-top: 10px;
}
.btngroup{
    display: flex;
    flex-direction: row;
}
.item{
    width: 160rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    border-radius: 3px;
    margin: 10rpx;
    margin-top: 10px;
    text-shadow: 0px 1px 1px rgba(0, 0, 0, 0.3)
}
.green{
    color: #ffffff;
    background-color: #4ebf22;
    border: 1px solid #4eee00;
}
.zero{
    width: 340rpx;
}